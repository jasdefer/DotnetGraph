<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Justus Bonz</Authors>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags></PackageTags>
    <Description>This library is a collection of algorithms running on directed or undirected graphs.
It is very easy to create the graph before running many differnt algorithms.</Description>
    <PackageProjectUrl>https://github.com/jasdefer/DotnetGraph/</PackageProjectUrl>
    <PackageId>DotnetGraphCore</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Algorithms\ShortestPath\**" />
    <EmbeddedResource Remove="Algorithms\ShortestPath\**" />
    <None Remove="Algorithms\ShortestPath\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
